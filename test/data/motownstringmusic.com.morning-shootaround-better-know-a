  <style type="text/css">.nav-head li a:hover{background-color:#006BB7;}.container{background-color:#001C44;}.pane h3,.entries h3.subtitle{background-color:#CE0022;}.col-side .sports_data_widget h3{border-color:#006BB7;}.nav-head-div{background-color:#FE002C;}* html .nav-head #nav-roster,* html .nav-head #nav-schedule,* html .nav-head #nav-stats,* html .nav-head #nav-news{background-image:none;background-color:#FE264C;}.gcolumns .col-side #thumbs a:hover img{border-color:#FE264C;}.col-side .sports_data_widget .pane-tabs li{background-color:#2681C2;}.sports_data_widget,.col-side .sports_data_widget,.col-side2 .sports_data_widget,.col-side .sports_data_widget .pane-tabs li a.active,.col-side .sports_data_widget .pane-tabs li a.active:visited,.col-side .sports_data_widget .pane-tabs li a.active:hover,.col-side .sports_data_widget .pane-tabs li a.active:active{background-color:#006BB7;}.entries .entry table.zebra tr.even td{background-color:#006BB7;}* html .col-side .sports_data_widget .zebra tr.even td,* html .col-side .sports_data_widget .zebra tr.even td.td-first,* html .col-side .sports_data_widget .zebra tr.even td.td-last,* html .sdw-rec-score p span,* html .sdw-rec-score p,* html .col-side .pane .next_game,* html .pane .foot .link-standings,* html .pane .foot .link-standings a{background-color:#2681C2;}a,a:link,a:visited,a:hover,a:active,a:focus,.entries .sports_data_widget a,.entries .sports_data_widget a:link,.entries .sports_data_widget a:visited,.entries .sports_data_widget a:hover,.entries .sports_data_widget a:active,.entries .sports_data_widget a:focus,.search .button,.title-bar h1 a,.title-bar h1 a:visited{color:#006BB7;}#yahoo_action_bar span.y-addtomy,.user_tools .user-tease-body span{border-color:#596B85;}#yahoo_action_bar span.y-addtomy a,.user_tools .user-tease-body span a,#gallery-mode a:hover{background-color:#006BB7;}.sports_data_widget .blog-hover-menu span a,.sports_data_widget .blog-hover-menu span a:link,.sports_data_widget .blog-hover-menu sp