ries .cform table:hover tr:hover,.col-content .entries .cform table:hover tr:hover td{background-color:none!important;}.fanshot p.footline,.nav-scope{background-color:#596B85!important;}.results-bar,.nav-scope-fs,.entries .entry table.zebra tr.even td{background-color:#D9DDE3!important;}.list-set p.even{background-color:#EBEDF0!important;}blockquote,div.blockquote,.cform,.cform input.textfield,.cform input[type="text"],.cform input[type="password"],.cform textarea{border-color:#596B85!important;}</style>  

